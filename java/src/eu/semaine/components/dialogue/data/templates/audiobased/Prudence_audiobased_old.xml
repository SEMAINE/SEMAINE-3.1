	<template id="Prudence_AudioBased_Sad1" name="Prudence">
	   <preconditions>
	     <compare value1="$Agent.character" value2="Prudence" />
	     <indicator startvalue="0" comparator="greater_equals" endvalue="8">
			<icompare value1="$UserTurn.AudioFeatures.LOGEnergy_avg" comparator="greater_equals" value2="-12.28595" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.RMSEnergy_max" comparator="lesser_equals" value2="0.1647" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.LOGEnergy_min" comparator="greater_equals" value2="-21.4205" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.arousal_max" comparator="lesser_equals" value2="-0.34" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.RMSEnergy_avg" comparator="greater_equals" value2="0.0065" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.arousal_max" comparator="greater_equals" value2="-0.46" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.valence_avg" comparator="lesser_equals" value2="-0.1125" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.intensity_max" comparator="lesser_equals" value2="-0.53" modifier="+1" />
	      	<icompare value1="$UserTurn.AudioFeatures.interest_max" comparator="greater_equals" value2="-0.957" modifier="+1" />
	     </indicator>
	     <trigger value1="$Agent.speakingIntention" value2="want_turn" />
	   </preconditions>
	   
	   <behaviour class="ResponseActionProposer" quality="0.9">
	    	<argument name="response" value="#Prudence62" />
	   </behaviour>
	</template>
	
