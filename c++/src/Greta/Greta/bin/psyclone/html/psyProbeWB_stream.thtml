document.getElementById){		//thisbrowser="NN6";		theimage = document.getElementById(image);		theimage.style.visibility="visible";	}}function HideImage(image) {//  alert("Hiding image " + image);	if(document.images){		//thisbrowser="NN4";		theimage = document.images[image];		theimage.visibility="hidden";	}	if(document.all){		//thisbrowser="ie"		theimage = document.all[image];		theimage.style.visibility="hidden";	}	if(!document.all && document.getElementById){		//thisbrowser="NN6";		theimage = document.getElementById(image);		theimage.style.visibility="hidden";	}}function ShowFrame(frame) {	for (var x = 1; x <= nframes; x++) {		if (x == frame) {      ShowLayer("layer" + x);    }    else      HideLayer("layer" + x);  }  cframe = frame;  vframe = frame;}function ViewFrame(frame) {	for (var x = 1; x <= nframes; x++) {		if (x == frame) {      ShowLayer("layer" + x);    }    else      HideLayer("layer" + x);  }  vframe = frame;}function HideAllFrames() {	for (var x = 1; x <= nframes; x++) {    HideLayer("layer" + x);  }}function GetNextFrameNumber() {  var n = cframe + 1;  if (n > nframes)    n = 1;  return n;}function GetPreviousFrameNumber() {  var n = cframe - 1;  if (n < 1)    n = nframes;  return n;}function GetNextViewNumber() {  var n = vframe + 1;  if (n == cframe+1)    return vframe;  if (n > nframes)    n = 1;  if (n == cframe+1)    return vframe;  return n;}function GetPreviousViewNumber() {  var n = vframe - 1;  if ((n == cframe) || (n == cframe+1))    return vframe;  if (n < 1)    n = nframes;  if ((n == cframe) || (n == cframe+1))    return vframe;  return n;}function GetViewNumber() {  if (vframe <= cframe)    return vframe - cframe;  else    return cframe - vframe;}function ShowNextFrame() {  ShowFrame(GetNextFrameNumber());}function ShowPreviousFrame() {  ViewFrame(GetPreviousFrameNumber());}function ViewNextFrame() {  ViewFrame(GetNextViewNumber());}function ViewPreviousFrame() {  ViewFrame(GetPreviousViewNumber());}function ImageLoadError() {//  alert("Image Loading Error");//  ShowImage('offline');//  if (isLiveUpdatingNow)//    switchStreamUpdate();}function ImageLoadComplete() {//  HideImage('offline');  n = GetNextFrameNumber();  // alert("Loaded image " + n + "...");  ShowNextFrame();}function LoadNextImage() {  n = GetNextFrameNumber();  var elem = document.getElementById('sampleimage' + n);  if (elem == null)    return;  if (imageSrc.length == 0)    imageSrc = elem.src;  // alert(imageSrc)  if ((imageSrc.length == 0) || (imageSrc.indexOf("wb=")==-1))    refreshContentPage();  var rand_no = Math.random();  elem.src = imageSrc + "&&nocache=" + rand_no;}function switchStreamUpdate() {//  HideImage('offline');	isLiveUpdatingNow = (!isLiveUpdatingNow);	if (isLiveUpdatingNow) {		document.images.monitorstatus.src = "images/continuousUpdateOn.gif";   	LiveStreamUpdate();	}	else {		document.images.monitorstatus.src = "images/continuousUpdateOff.gif";	}}function LiveStreamUpdate() {  if (!isLiveUpdatingNow)    return;  LoadNextImage();//  var elem = document.getElementById('sampleimage');//  var elem = document.layer[cframe+1].images[0]; // getElementById('sampleimage');//  if (elem == null)//    return;//	elem.src = elem.src;//  if (isLiveUpdatingNow)//    timerID=setTimeout("LiveStreamUpdate()", 1000); }function refreshContentPage() {//   alert(document.myform.timeout.selectedIndex);   document.location=document.location;//   document.myform.submit();}function stopErrors() {   return true;}function launchPage(thePage) {   window.open(thePage);}function launchPostMessage(thePage) {window.open(thePage,'Post_Message','width=760,height=700,toolbar=yes,resize=yes,scrollbars=yes,directories=no,menubar=no,status=no'); }// getPsycloneID(CallbackID)function CallbackID() {	var id = "";	var myajax=psyJax.ajaxobj;	var myfiletype=psyJax.filetype;	//if request of file not yet completed  if (myajax.readyState != 4)    return;  try {    //if request was successful or running script locally    if (myajax.status==200 || window.location.href.indexOf("http")==-1) { 		  if (myfiletype=="txt") {        id = myajax.responseText;		  }	  }	  else {		  // alert("Error: " + myajax.status);	  }  } catch (e) {}  if (id.length == 0) {    // alert("No ID: " + myajax.readyState);    ShowImage('offline');    //if (isLiveUpdatingNow)    //  switchStreamUpdate();  }  else {    // alert("ID: " + id);    SystemID = id;    HideImage('offline');  }}//window.onerror = stopErrors;//--></script></head><body text="#660000" link="#990000" vlink="#990000" alink="#CC0000" leftmargin="20" topmargin="20" bottommargin="0" onload="ShowFrame(1);Main();"><STYLE>A:link {   TEXT-DECORATION: none}A:visited {   TEXT-DECORATION: none}A:active {   TEXT-DECORATION: none}A:hover {   COLOR: #cccc99}</STYLE><div align="center"></div><table width="888" border="0" valign="bottom" align="center" cellpadding="0" cellspacing="0">  <tr>     <td width="152" height="38" rowspan="3" valign="bottom"><img src="psyclonePsyProbeLogo3.gif" align="bottom"></td>    <td width="196" align="left" valign="bottom">&nbsp;</td>    <td width="212" align="left" valign="bottom"><div align="right"><span class="navLink"><a href="streamplayer.html" target="_blank" class="navLink">         stream_player_:</a>:</span></div></td>    <td width="161" height="19" align="left" valign="bottom"> <div align="right" class="navLink"><a href="JavaScript:launchPage('index.html');">documentation_:</a>:</div></td>    <td width="167" align="right" valign="bottom" class="navLink"><a href="JavaScript:launchPage('psycloneManual.html#wbStreamPage:about');">about_this_page_:</a>:</td>  </tr>  <tr>     <td colspan="4" align="left" valign="bottom"><img src="images/simple-circle-line-1-short.gif" width="536" height="11"><img src="images/li